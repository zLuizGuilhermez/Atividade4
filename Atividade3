#include <stdio.h>

main()
{

    int a = 0, b = 0, c = 0;

    printf("Digite 3 numeros");
    scanf("%d %d %d", &a, &b, &c);

    if (a < b && a < c)
    {
        printf("\nO  menor numero e %d", a);
    }
    if (a > b && a > c)
    {
        printf("\no seu maior numero e %d", a);
    }
    if (a > b && a < c)
    {
        printf("\nO numero do meio e %d", a);
    }
    if (a < b && a > c)
    {
        printf("\nO numero do meio e %d", a);
    }

    if (b < a && b < c)
    {
        printf(" \nO  menor numero e %d", b);
    }
    if (b > a && b > c)
    {
        printf("\no seu maior numero e %d", b);
    }
    if (b > a && b < c)
    {
        printf("\nO numero do meio e %d", b);
    }
    if (b < a && b > c)
    {
        printf("\nO numero do meio e %d", b);
    }

    if (c < a && c < b)
    {
        printf("\nO  menor numero e %d", c);
    }
    if (c > a && c > b)
    {
        printf("\no seu maior numero e %d", c);
    }
    if (c > a && c < b)
    {
        printf("\nO numero do meio e %d", c);
    }
    if (c < a && c > b)
    {
        printf("\nO numero do meio e %d", c);
    }
}
